generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum SeverityLevel {
  LOW
  MEDIUM
  HIGH
}

model LogModel {
  id        Int           @id @default(autoincrement())
  level     SeverityLevel
  message   String
  origin    String
  createdAt DateTime      @default(now())
}
